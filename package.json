  "repository": {
    "type": "git",
    "url": "git://github.com/stronkmagic/angular-cron-editor"
  },
  "bugs": {
    "url": "https://github.com/stronkmagic/angular-cron-editor/issues"
  },
  "homepage": "https://github.com/stronkmagic/angular-cron-editor",
  "keywords": [
    "angular",
    "cron",
    "cron-expression",
    "angular-cli",
    "quartz"
  ],
  "scripts": {
    "ng": "ng",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "build_lib": "ng build angular-cron-editor",
    "build": "ng build angular-cron-editor && ng build --prod",
    "start": "yarn run build_lib && ng serve",
    "copy-license": "copy .\\LICENSE.md .\\dist\\angular-cron-editor",
    "copy-readme": "copy .\\README.md .\\dist\\angular-cron-editor",
    "copy-files": "npm run copy-license && npm run copy-readme",
    "pack_lib": "cd dist/angular-cron-editor && npm pack",
    "package": "npm run build_lib && npm run copy-files && npm run pack_lib"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~7.1.1",
